script/source = "extends \"res://scripts/generic_character_behaviour.gd\"

func _ready():
	if (!get_tree().root.is_queued_for_deletion()):
		._setup_collision() # set up collision shapes
#		ammo = 10 # starting ammo
#		energy = 0 # starting energy
#		health = 100 # starting health
#		mainCharacter = true # is main character
#		action1Damage = 40
#		action2Damage = 20
#		action3Damage = 10
		velocity = Vector2(0, 0)
		facingDirection = 1
		movementDirection = 1
		
		playerSprite = get_node(\"AnimatedSprite\")
		._change_sprite_animation(\"idle\")
		bullet_scene = preload(\"res://scenes/characters/robot/Bullet_Scene.tscn\")
		type = \"robot\"
		blood_colour = Color(\"#0a0a0a\") # black
		
		._emit_character_ready()
		._subscribe_to_signals()
		print(\":: \" + type + \" character spawed ::\")
		set_process(true)
	pass


func _process(delta):
	._start_process(delta)
	# check for character control
	control_character(delta)
	pass
		

func control_character(delta):
	# reset default behaviour
	._reset_character_sprite_states(delta)
	
	# Handle input events
	if (!dazed and health > 0):
		if (Input.is_action_just_pressed(\"action_1\") and !action1 and !action2 and !action3 and ammo > 0):
			._shoot_bullet(2)
			repeatFrames = false;
			
			if (currentJumpCount > 0):
				._change_sprite_animation(\"jump_shoot\")
			elif (movementDirection != 0):
				._change_sprite_animation(\"run_shoot\")
			elif (movementDirection == 0):
				._change_sprite_animation(\"shoot\")
		
			._emit_refresh_hud()
		elif (Input.is_action_just_pressed(\"action_2\") and !action1 and !action2 and !action3):
			action2 = true
			repeatFrames = false;
			._melee_attack_collision()
			if (currentJumpCount > 0):
				._change_sprite_animation(\"jump_melee\")
			else:
				._change_sprite_animation(\"melee\")
				
		elif (Input.is_action_just_pressed(\"action_3\") and movementDirection != 0 and !action3):
			action3 = true
			repeatFrames = false;
			._change_sprite_animation(\"slide\")
			._slide_attack_collision()
		elif (Input.is_action_just_pressed(\"move_jump\") and currentJumpCount < maxJumpCount and !action3):
			playerSpeedY = -JUMPFORCE
			currentJumpCount += 1
			._change_sprite_animation(\"jump\")
			playerSprite.frame = 0
			repeatFrames = false
			action1 = false
			action2 = false
			action3 = false
		elif (Input.is_action_pressed(\"move_left\")):
			._move_left()
		elif (Input.is_action_pressed(\"move_right\")):
			._move_right()
		elif (Input.is_action_pressed(\"respawn\")):
			._emit_reposition()
		else:
			movementDirection = 0
			if (health <= 0):
				._change_sprite_animation(\"dead\")
				repeatFrames = false
			elif (currentJumpCount == 0 and !action1 and !action2 and !action3):
				._change_sprite_animation(\"idle\")
				repeatFrames = true
	pass"

